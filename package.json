{
  "name": "tdd-fastify-traning-2021-12-15",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test" :  "cross-env NODE_ENV=test jest --runInBand",
    "test:coverage" :  "cross-env NODE_ENV=test jest --runInBand --coverage",
    "test:watch" :  "cross-env NODE_ENV=test jest --watch --runInBand",
    "test:watch-verbose": "cross-env NODE_ENV=test jest --watch --runInBand --verbose",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "cross-env NODE_ENV=develop nodemon",
    "dev:test": "cross-env NODE_ENV=test nodemon",
    "lint": "eslint --fix {src,test}/**/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.14.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.0.0",
    "@types/tap": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^6.0.0",
    "config": "^3.3.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "fastify-tsconfig": "^1.0.1",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "tap": "^15.0.9",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  }
}
